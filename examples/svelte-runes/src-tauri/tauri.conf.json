{
  "productName": "example-svelte-runes",
  "identifier": "com.tbdev.example",
  "build": {
    "beforeDevCommand": "pnpm run -F example-svelte-runes vite:dev",
    "beforeBuildCommand": "pnpm run -F example-svelte-runes vite:build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1422"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "assetProtocol": {
        "enable": true
      },
      "csp": {
        "default-src": "'self'",
        "connect-src": "ipc: http://ipc.localhost"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../../assets/icons/32x32.png",
      "../../assets/icons/128x128.png",
      "../../assets/icons/128x128@2x.png",
      "../../assets/icons/icon.icns",
      "../../assets/icons/icon.ico"
    ]
  }
}
