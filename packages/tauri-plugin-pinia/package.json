  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "typedoc": "typedoc --plugin typedoc-plugin-mdn-links"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.3",
    "@tb-dev/utils": "^4.0.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.4",
    "vue": "^3.5.12"
