{
  "name": "tauri-plugin-pinia",
  "version": "0.8.0",
  "description": "Persistent Pinia stores for Tauri",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@9.12.3",
  "homepage": "https://tb.dev.br/tauri-store",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/tauri-store.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/tauri-store/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "vue",
    "tauri",
    "pinia",
    "plugin"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
